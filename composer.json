{
  "name": "braunstetter/template-hooks-bundle",
  "type": "symfony-bundle",
  "description": "Give your bundles a chance to extend your templates.",
  "license": "MIT",
  "authors": [
    {
      "name": "Slowwie",
      "email": "michaelbrauner82@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/twig-bundle": "5.3.*",
    "symfony/yaml": "5.3.*"
  },
  "autoload": {
    "psr-4": {
      "Braunstetter\\TemplateHooks\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Braunstetter\\TemplateHooks\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/test-pack": "^1.0"
  }
}
